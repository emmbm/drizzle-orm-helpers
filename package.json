{
	"author": "Emmanuel Beaudry Marchand",
	"description": "A collection a tiny helpers for Drizzle ORM.",
	"license": "MIT",
	"type": "module",
	"name": "drizzle-orm-helpers",
	"version": "0.1.5",
	"files": [
		"dist",
		"package.json",
		"README.md"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.40.0",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-drizzle": "^0.2.3",
		"prettier": "^3.2.5",
		"prettier-plugin-jsdoc": "^1.3.0",
		"tsup": "^8.0.1",
		"type-fest": "^4.10.2",
		"typedoc": "^0.25.7",
		"typedoc-plugin-markdown": "^3.17.1",
		"typescript": "^5.3.3"
	},
	"peerDependencies": {
		"drizzle-orm": "^0.29.3"
	},
	"scripts": {
		"doc": "pnpm typedoc --plugin typedoc-plugin-markdown --hideBreadcrumbs true --out docs src/index.ts && mv docs/modules.md documentation.md && rm -rf docs",
		"prepublishOnly": "pnpm doc && pnpm tsup"
	}
}