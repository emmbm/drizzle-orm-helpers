{
	"author": "Emmanuel Beaudry Marchand",
	"description": "A collection a tiny helpers for Drizzle ORM.",
	"license": "MIT",
	"type": "module",
	"name": "drizzle-orm-helpers",
	"version": "0.3.1",
	"files": [
		"dist",
		"package.json",
		"README.md",
		"documentation.md"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/iolyd/drizzle-orm-helpers.git"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-drizzle": "^0.2.3",
		"prettier": "^3.2.5",
		"prettier-plugin-jsdoc": "^1.3.0",
		"remark-github": "^12.0.0",
		"remark-toc": "^9.0.0",
		"tsup": "^8.0.1",
		"type-fest": "^4.10.2",
		"typedoc": "^0.25.7",
		"typedoc-plugin-markdown": "4.0.0-next.53",
		"typedoc-plugin-remark": "0.0.1-next.2",
		"typescript": "^5.3.3",
		"unified-prettier": "^2.0.0"
	},
	"peerDependencies": {
		"drizzle-orm": "^0.29.3"
	},
	"scripts": {
		"doc": "pnpm typedoc && mv doc/documentation.md documentation.md && rm -rf doc",
		"prepublishOnly": "pnpm tsup && pnpm doc"
	}
}